# JWT Secret for signing tokens
JWT_SECRET=generate-with-openssl-rand-base64-64

# Valid API Keys (comma-separated)
VALID_API_KEYS=your-api-key-1,your-api-key-2

# Application URLs
# For development (emulator):
# APP_BASE_URL=http://127.0.0.1:5000
# EMAIL_LINK_URL=http://127.0.0.1:5000
# For production:
APP_BASE_URL=https://your-app.web.app
EMAIL_LINK_URL=https://your-app.web.app

# Environment
NODE_ENV=production

# Data Connect URL (optional - SDK auto-detects)
DATACONNECT_URL=https://firebasedataconnect.googleapis.com/v1alpha/projects/your-project/locations/us-central1/services/mybox-dataconnect
