# Cloud Functions Base URL
VITE_CLOUD_FUNCTIONS_BASE_URL=https://us-central1-your-project-id.cloudfunctions.net

# Cloud Function Endpoints
VITE_CF_GENERATE_AUTH_LINK=/generateAuthLink
VITE_CF_VALIDATE_SESSION=/validateSession
VITE_CF_PROCESS_UPLOAD=/processUpload
VITE_CF_LIST_FILES=/listFiles
VITE_CF_DELETE_FILE=/deleteFile
VITE_CF_GENERATE_DOWNLOAD_URL=/generateDownloadURL
VITE_CF_BATCH_DOWNLOAD_URLS=/batchGenerateDownloadURLs

# Test API Key (for simulating third-party service)
# Use this in development to test the generateAuthLink endpoint
VITE_TEST_API_KEY=test-api-key-12345

# Application Settings
VITE_APP_NAME=MyBox
VITE_APP_ENV=development

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Firebase Cloud Functions URL (for development or production)
VITE_FIREBASE_FUNCTIONS_URL=https://us-central1-your-project-id.cloudfunctions.net

# Third-Party API Configuration (for backend functions)
# These are used by Cloud Functions, not the frontend
# Add to Cloud Functions environment variables via Firebase Console
# VALID_API_KEYS=key1,key2,key3
# EMAIL_LINK_URL=https://your-app-domain.com/auth/verify
# APP_BASE_URL=https://your-app-domain.com
